use social_media;

create table users(
    id integer primary key auto_increment,
	name varchar(60),
    profile varchar(70),
    password varchar(60),
    headline varchar(100),
    token varchar(600)
);

create table posts(
    profile varchar(50),
    content varchar(6000),
    likes integer,
    shares integer



);

